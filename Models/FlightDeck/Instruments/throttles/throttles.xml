<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>throttles.ac</path>

  <animation>
    <object-name>Mixture</object-name>
    <object-name>PropPitch</object-name>
    <object-name>Throttle</object-name>
  </animation>


<!-- Cold and Dark Unlighted -->

  <animation>
  <type>material</type>
    <object-name>Mixture</object-name>
    <object-name>PropPitch</object-name>
    <object-name>Throttle</object-name>
    <emission>
      <red>   .015 </red>
      <green> .015 </green>
      <blue>  .015 </blue>
      <factor-prop>/sim/time/sun-angle-rad</factor-prop>
    </emission>
    <condition>
	    <greater-than>
          <property>/sim/time/sun-angle-rad</property>
          <value>1.55</value>
        </greater-than>
    </condition>
  </animation>

<!-- Cold and Dark Lighted -->


<!-- Instrument Lights On -->

<!-- End of Emission Animations -->






</PropertyList>
