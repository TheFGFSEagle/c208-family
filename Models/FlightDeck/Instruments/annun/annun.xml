<?xml version="1.0"?>
<PropertyList>

  <path>annunpanel.ac</path>

<!-- Cold and Dark Unlighted -->

 <animation>
  <type>material</type>
  <object-name>annunpanel</object-name>
  
    <emission>
      <red>   .011 </red>
      <green> .011 </green>
      <blue>  .011 </blue>
      <factor-prop>/sim/time/sun-angle-rad</factor-prop>
    </emission>
    <condition>
      <and>
	    <greater-than>
          <property>/sim/time/sun-angle-rad</property>
          <value>1.55</value>
        </greater-than>
        <not>
          <property>controls/lighting/flashlight</property>
        </not>
      </and>
    </condition>
  </animation>

<!-- Flashlight -->

 <animation>
  <type>material</type>
  <object-name>annunpanel</object-name>
  
    <emission>
      <red>   .1 </red>
      <green> .1 </green>
      <blue>  .1 </blue>
      <factor-prop>/controls/lighting/flashlight</factor-prop>
    </emission>
     <condition>
     <and>
      <property>/controls/lighting/flashlight</property>
   	    <greater-than>
          <property>/sim/time/sun-angle-rad</property>
          <value>1.55</value>
        </greater-than>
      </and>
    </condition>
  </animation>


<!-- Cold and Dark Lighted -->

