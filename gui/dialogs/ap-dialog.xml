<PropertyList>
    <name>autopilot</name>
    <layout>vbox</layout>

  <x>20</x>
  <y>20</y>


  <group>
    <layout>hbox</layout>
    <empty><stretch>1</stretch></empty>

    <text>
      <label>Cessna-208B Autopilot</label>
    </text>

    <empty><stretch>1</stretch></empty>

    <button>
      <pref-width>16</pref-width>
      <pref-height>16</pref-height>
      <legend></legend>
      <default>1</default>
      <keynum>27</keynum>
      <border>2</border>
      <binding>
        <command>dialog-apply</command>
      </binding>
      <binding>
          <command>nasal</command>
          <script>dialogs.ap_settings.toggle()</script>

      </binding>
    </button>
  </group>

  <hrule>
    <color>
      <red>1.0</red>
      <green>0.0</green>
      <blue>0.0</blue>
    </color>
    <pref-height>1</pref-height>
  </hrule>

<group>
  <layout>hbox</layout>

        <group>
          <layout>vbox</layout>
          <!-- Pitch Trim Buttons -->
          <button>
            <pref-width>40</pref-width>
            <pref-height>45</pref-height>
            <legend>Down</legend>
            <binding>
              <condition>
                <property>/autopilot/internal/VNAV-ALT</property>
              </condition>
              <command>property-adjust</command>
              <property>/autopilot/settings/target-altitude-ft</property>
              <step>-100.0</step>
              <wrap>false</wrap>
            </binding>
            <binding>
              <condition>
                <not>
                  <property>/autopilot/internal/VNAV-ALT</property>
                </not>
              </condition>
              <command>property-adjust</command>
              <property>/autopilot/internal/last-pitch</property>
              <step>-0.5</step>
              <wrap>false</wrap>
            </binding>
          </button>

          <button>
            <pref-width>40</pref-width>
            <pref-height>45</pref-height>
            <legend>Up</legend>
            <binding>
              <condition>
                <property>/autopilot/internal/VNAV-ALT</property>
              </condition>
              <command>property-adjust</command>
              <property>/autopilot/settings/target-altitude-ft</property>
              <step>100.0</step>
              <wrap>false</wrap>
            </binding>
            <binding>
              <condition>
                <not>
                  <property>/autopilot/internal/VNAV-ALT</property>
                </not>
              </condition>
              <command>property-adjust</command>
              <property>/autopilot/internal/last-pitch</property>
              <step>0.5</step>
              <wrap>false</wrap>
            </binding>
          </button>
        </group>

